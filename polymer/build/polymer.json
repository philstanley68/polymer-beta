{
  "entrypoint": "index.html",
  "shell": "src\\rn-app\\rn-app.html",
  "fragments": [],
  "sources": [
    "src\\**\\*",
    "Release_Notes_RN_SM_files\\**\\*",
    "css\\**\\*",
    "src\\rn-app\\pureconnect.svg",
    "src\\rn-app\\genesys-logo.svg",
    "bower.json",
    "src\\rn-app\\rn-beta.css",
